<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!-- Mapperとxmlのマッピング -->
<mapper namespace="com.portfolio.mapper.AssetMapper">

	<!-- Asset複数件取得 -->
	<select id="findAllByUserId">
		select
		*
		from
		asset
		where
		user_id
		=
		#{userId}
	</select>


</mapper>